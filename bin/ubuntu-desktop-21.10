#!/bin/sh
# Download verification:
isoFile=~/virt-machines/ubuntu-21.10-desktop-amd64.iso

gd=~/bin
scmFile=$gd/qemu-vm.scm
guild compile --load-path=$gd -W0 $scmFile 1>/dev/null
# guile doesn't have the long version of '-L' for '--load-path'
echo sudo guile -L $gd -e main $scmFile --user=$USER --iso-file=$isoFile $@
     sudo guile -L $gd -e main $scmFile --user=$USER --iso-file=$isoFile $@

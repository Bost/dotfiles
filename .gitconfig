# Add aliases to your ~/.gitconfig file:
[alias]
	au = add --update
	ss = status --short
	st = status
	ci = commit
	br = branch
	co = checkout
	df = diff
	bi = bisect
	lg = log -p
	who = shortlog -s --
	undo=reset --hard
[user]
	name = Bost
	email = thebost@gmail.com

[sendemail]
	smtpencryption = tls
	smtpserver = smtp.gmail.com
	smtpuser = Rostislav.Svoboda@gmail.com
	smtpserverport = 587

[color]
	ui = always
